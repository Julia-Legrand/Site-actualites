<form method="post" action="{{ path('app_contacts_delete', {'id': contact.id}) }}" onsubmit="return confirm('Êtes-vous sûre de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ contact.id) }}">
    <button class="icon"><i class="fa-regular fa-trash-can"></i></button>
</form>
